{
	"format_version": "1.10.0",
	"render_controllers": {
		"controller.render.dragon_general": {
			"arrays": {
				"textures": {
					"Array.base": [
						"Texture.dragon"
					],
					"Array.saddled": [
						"Texture.dragon_none",
						"Texture.dragon_saddled"
					],
					"Array.armor": [
						"Texture.dragon_none",
						"Texture.dragon_armor_iron",
						"Texture.dragon_armor_gold",
						"Texture.dragon_armor_diamond",
						"Texture.dragon_armor_emerald"
					]
				}
			},
			"part_visibility": [
				{
					"horn_front": "!query.is_saddled"
				}
			],
			"geometry": "Geometry.dragon_default",
			"materials": [
				{
					"*": "Material.dragon"
				}
			],
			"textures": [
				"Array.base[query.variant]",
				"Array.saddled[query.is_saddled]",
				"Array.armor[query.mark_variant]"
			]
		},
		"controller.render.dragon_egg": {
			"geometry": "Geometry.dragon_egg",
			"materials": [
				{
					"*": "Material.dragon_egg"
				}
			],
			"textures": [
				"Texture.dragon_egg"
			],
			"is_hurt_color": {
				"r": 1,
				"g": 1,
				"b": 1,
				"a": 0.1
			}
		}
	}
}